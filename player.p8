pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
p={x=64,y=120,dx=0,dy=0}
m={x=32,y=120,dx=0,dy=0}
next_arm_move=0
arm_pos=1
f=0
h=5
colliding=false
ground={}

function _init()
 y=121
 for x=0,(127/5) do
  y+=rnd(2)-1
  ground[flr(x)]=y
 end
end

function _update()
 p.dx*=.5
 m.dx*=.5

 if (btn(0)) p.dx=-1
 if (btn(1)) p.dx=1
 if (btn(2) and p.y==ground[flr(p.x/5)]-1) p.dy=-2
 
 dist=p.x-m.x
 if dist>1 then
  m.dx=.5
 elseif dist<-1 then
  m.dx=-.5
 end
 
 p.x+=p.dx
 p.y+=p.dy
 m.x+=m.dx
 m.y+=m.dy
 
 if p.y<ground[flr(p.x/5)]-1 then
  p.dy+=.2
 else
  p.y=ground[flr(p.x/5)]-1
  p.dy=0
 end
 if m.y<ground[flr(m.x/5)]-1 then
  m.dy+=.2
 else
  m.y=ground[flr(m.x/5)]-1
  m.dy=0
 end 
 
 if m.x>=p.x-1 and m.x<=p.x+1 and
    m.y<=p.y and m.y>=p.y-4 then
  if not colliding then
   h-=1
   colliding=true
  end
 else
  colliding=false
 end
 if h<=0 then
  print('you died!')
  _update=nil
  _draw=nil
 end
end

function draw_c(c,col,arm_pos)
 arm_pos=arm_pos and 1 or 0
 rectfill(c.x-1,c.y-1,c.x+1,c.y-4,col)
 pset(c.x-2,c.y-2-arm_pos,col)
 pset(c.x+2,c.y-2-arm_pos,col)

 a=-1
 b= 1
 if f%16>8 then
  if c.dx>.1 then
   a=-1 b= 2
  elseif c.dx<-.1 then
   a=-2 b= 1
  end
 end
 pset(c.x+a,c.y,col)
 pset(c.x+b,c.y,col)
 pset(c.x,c.y-3,0)
end

function _draw()
 cls()
 for x=0,(127/5) do
  rectfill(x*5,ground[flr(x)],x*5+5,ground[x],5)
 end

 draw_c(p,1,f%16>8)
 draw_c(m,2,f%16<=8)

 x=0
 for i=1,h do
  spr(1,x,1)
  x+=7
 end
 
 f+=1
end
